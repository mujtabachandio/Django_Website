{
    "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],

    "builds": [{
        "src": "djangoprojectname/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/wsgi.py"
        }
    ]
}